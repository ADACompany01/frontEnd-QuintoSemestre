{
  "expo": {
    "name": "ADA Company",
    "slug": "ada-company",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "icon": "./assets/AdaHome.png",
    "splash": {
      "image": "./assets/AdaHome.png",
      "resizeMode": "cover",
      "backgroundColor": "#667eea"
    },
    "splashScreen": {
      "hideExponentText": true,
      "hideInstructions": true
    },
    "loading": {
      "hideExponentText": true,
      "hideInstructions": true,
      "icon": "./assets/AdaHome.png"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/AdaHome.png",
        "backgroundColor": "#667eea"
      }
    },
    "web": {
      "bundler": "metro"
    },
    "platforms": ["ios", "android", "web"]
  }
}
