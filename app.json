{
  "expo": {
    "name": "ADA Company",
    "slug": "ada-company",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "icon": "./assets/AdaHome.png",
    "splash": {
      "image": "./assets/AdaHome.png",
      "resizeMode": "contain",
      "backgroundColor": "#667eea"
    },
    "splashScreen": {
      "hideExponentText": true,
      "hideInstructions": true,
      "resizeMode": "contain"
    },
    "loading": {
      "hideExponentText": true,
      "hideInstructions": true,
      "icon": "./assets/AdaHome.png",
      "backgroundColor": "#667eea"
    },
    "developer": {
      "tool": "expo-cli"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "splash": {
        "image": "./assets/AdaHome.png",
        "resizeMode": "contain",
        "backgroundColor": "#667eea"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/AdaHome.png",
        "backgroundColor": "#667eea"
      },
      "splash": {
        "image": "./assets/AdaHome.png",
        "resizeMode": "contain",
        "backgroundColor": "#667eea"
      }
    },
    "web": {
      "bundler": "metro"
    },
    "platforms": ["ios", "android", "web"]
  }
}
